@{
    ViewBag.Title = "searchResult";
    Layout = "~/Views/Shared/_LayoutView.cshtml";
}
@model KhuyenNong.Models.searchResult


<h2>Search result for "@Model.keysword"</h2>

<body>
    <!-- BEGIN wrapper -->
    <div id="wrapper">

        <!-- BEGIN content -->
        <div id="content">
            <!-- begin post -->
            <div class="single">
               @{
                   int length = Model.row.Count;
                   foreach (var i in Model.row)
                   {

                    
                                <h3><a href="#" style="color:blue">@i.Title</a></h3>
                     
                                <h5>@i.Contains.Substring(0, 256)</h5>
                            
                   }
            }
            </div>
            <!-- end post -->
        </div>
        <!-- END content -->


    </div>
    <!-- END wrapper -->
</body>